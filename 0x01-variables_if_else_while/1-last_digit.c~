#include <stdlib.h>
#include <time.h>
#include <stdio.h>

/*Program to print the last digit of a number*/
int main(void)
{
int n;

 srand(time(0));
 n = rand() - RAND_MAX / 2;
 printf("Enter a number: \n");
 scanf("%d", &n);
 digit = n%10
   if (digit > 5)
     printf("Last digit of %d is %d"/*/*/greater than 5 \n", digit);
 
 return (0);
